% layout 'default';
% title 'Welcome';

<table class="table table-condensed">
<thead>
<tr>
    % for my $col (@$columns) {
    <th>
        %= $col
    </th>
    % }
</tr>
</thead>
% while (my $row = $rows->next) {
<tr>
    % for my $col (@$columns) {
      <td>
        %# Load column type template
        %= $row->get_column($col);        
      </td>
    % }        
</tr>
% }
</table>